<?xml version="1.0" encoding="UTF-8"?>
<project name="${project.name}" xmlns="http://maven.apache.org/DECORATION/1.0.0"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">
    <bannerLeft>
        <name>${project.name}</name>
        <href>${project.url}</href>
        <src>images/em-logo-250x80.jpg</src>
    </bannerLeft>
    <bannerRight>
        <name>Advanced Natural Research Institute Projects</name>
        <src>images/anri_logo_48.png</src>
        <href>https://project.anrisoftware.com/</href>
    </bannerRight>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.7</version>
    </skin>
    <body>
        <links>
            <item name="${project.name}" href="${project.url}"/>
        </links>
        <menu name="Overview">
            <item name="Home" href="/index.html" />
            <item name="Download (Central)" href="https://search.maven.org/artifact/${project.groupId}/${project.artifactId}/${project.version}/pom" />
            <item name="Source code" href="${project.custom.scm.repo}" />
            <item name="Source code (Github)" href="${project.custom.scm.github}" />
            <item name="Project Home" href="${project.issueManagement.url}" />
            <item name="Project Roadmap" href="${project.issueManagement.url}/roadmap" />
            <item name="Project Issues" href="${project.issueManagement.url}/issues" />
            <item name="Jenkins" href="${project.custom.jenkins.url}" />
            <item name="SonarQube" href="${project.custom.sonarqube.url}" />
        </menu>
        <menu ref="parent" />
        <menu ref="modules" />
        <menu ref="reports" />
    </body>
</project>
